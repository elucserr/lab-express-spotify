<section class="albums">
    {{#each albums}}
    <div class="album">
        <img class="album-image" src="{{this.images.[0].url}}" alt="">
        <div class="album-name">
            <p>{{this.name}}</p>
            <a class="album-button" href="/tracks/{{this.id}}">View Tracks</a>
        </div>
    </div>
    {{/each}}
</section> 